{
  "expo": {
    "name": "AttTrack",
    "slug": "AttTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-1024x1024.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "atttrack",
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atttrack",
      "icon": "./assets/logo-1024x1024.png", 
      "splash": {
        "image": "./assets/logo-1024x1024.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-1024x1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atttrack",
      "splash": {
        "image": "./assets/logo-1024x1024.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    },
    
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/logo-1024x1024.png",
          "imageWidth": 200 
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b3712540-b0f9-48ed-8fa4-102b2fa2fa95"
      }
    }
  }
}
