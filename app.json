{
  "expo": {
    "name": "AttTrack",
    "slug": "AttTrack",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/logo-1024x1024.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "atttrack",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atttrack"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-1024x1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atttrack"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-1242x2688.png",
          "resizeMode": "contain"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b3712540-b0f9-48ed-8fa4-102b2fa2fa95"
      }
    }
  }
}
